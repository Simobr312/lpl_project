complex Triangle = [A1, A2, A3]

complex Tet = [B1, B2, B3, B4]

// Boundary: union of all triangular faces
complex F1 = [A, B, C]
complex F2 = [A, B, D]
complex F3 = [A, C, D]
complex F4 = [B, C, D]

// Union all faces to form the boundary
complex TetBoundary = union(union(F1, F2), union(F3, F4))

complex K1 = glue(Triangle, Tet) mapping A1 -> B1
complex K = glue(K1, TetBoundary) mapping A1 -> A 