//Just a "crystal" with 2-dimensional holes
complex X = [v0]

//You can change the number to whatever you want
while leq(num_vert(X), 30) do
    vertex v
    complex Y = union([v0], v)
    while sub(num_vert(Y), 6) do
        vertex w
        vertex z
        Y <- join(Y, union(w,z))
    endwhile
    X <- union(X, Y)
endwhile
